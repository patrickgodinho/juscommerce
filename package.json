{
  "name": "juscommerceapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "relay": "relay-compiler --src ./src/ --schema ./data/schema.graphql --language typescript --artifactDirectory ./__generated__/"
  },
  "dependencies": {
    "graphql": "^14.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "0.59.8",
    "react-relay": "^4.0.0",
    "relay-runtime": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/enzyme": "^3.9.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.57.55",
    "@types/react-test-renderer": "^16.8.1",
    "@types/relay-runtime": "^1.3.1",
    "babel-jest": "^24.8.0",
    "babel-preset-react-native": "^4.0.1",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-relay": "^4.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3",
    "relay-compiler": "^4.0.0",
    "relay-compiler-language-typescript": "^4.2.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
